{
    "name": "nestjs-puppeteer",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "test": "jest",
        "build": "nest build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AgusNeira/nestjs-puppeteer.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AgusNeira/nestjs-puppeteer/issues"
    },
    "homepage": "https://github.com/WadiahCapital/nestjs-puppeteer#readme",
    "dependencies": {
        "@nestjs/common": "^9.1.2",
        "puppeteer": "^19.8.0",
        "puppeteer-extra": "^3.3.0",
        "puppeteer-extra-plugin-stealth": "^2.9.0"
    },
    "peerDependencies": {
        "@nestjs/core": "^9.1.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^9.1.2",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.1",
        "jest": "^27.3.0",
        "ts-jest": "^27.0.7",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.3.0",
        "typescript": "^4.4.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/src/$1",
            "^@/(.*)$": "<rootDir>/src/$1",
            "puppeteer-core/internal/(.*)": "puppeteer-core/lib/cjs/puppeteer/$1"
        },
        "rootDir": "./",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}